{
  "name": "challengeoftheday",
  "version": "1.0.0",
  "description": "Theme generator for the #challenges channel of the MadisonGameDev Slack Group",
  "main": "index.js",
  "scripts": {
    "test": "nyc mocha",
    "coverage": "nyc report --reporter=text-lcov > lcov.info",
    "clean": "rimraf build",
    "build": "npm run clean && babel src -d build/src && babel index.js -d build/",
    "lint": "eslint index.js test/** src/**",
    "start": "node index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hartalex/ChallengeOfTheDay.git"
  },
  "author": "Alex Hart",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/hartalex/ChallengeOfTheDay/issues"
  },
  "homepage": "https://github.com/hartalex/ChallengeOfTheDay#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-minify": "^0.2.0",
    "codeclimate-test-reporter": "^0.5.1",
    "eslint": "^4.7.2",
    "eslint-config-standard": "10.2.1",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-promise": "3.5.0",
    "eslint-plugin-standard": "3.0.1",
    "mocha": "^3.5.3",
    "nyc": "^11.2.1",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "request": "2.83.0",
    "twit": "2.2.9"
  }
}
